<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_selected="true"><layer-list>
            <item><shape>

                    <!-- 描边 -->
                    <stroke android:dashGap="2.0dip" android:dashWidth="4.0dip" android:width="1.0dip" android:color="@color/contents_color" />
                    <!-- 实心颜色 -->
                    <solid android:color="@color/white" />

                    <corners android:radius="5dp" />
                </shape></item>
        </layer-list></item>
    <item android:state_selected="true"><shape>
            <stroke android:width="1.0dip" android:color="@color/contents_color" />

            <corners android:radius="5dp" />

            <solid android:color="@color/white" />
        </shape></item>
    <item android:state_enabled="false"><shape>
            <stroke android:width="0.5dip" android:color="@color/subscribe_item_disabled_stroke" />

            <corners android:radius="5dp" />

            <solid android:color="@color/subscribe_item_disabled_bg" />
        </shape></item>
    <item android:state_pressed="true"><shape>
            <stroke android:width="0.5dip" android:color="@color/contents_color" />

            <corners android:radius="5dp" />

            <solid android:color="@color/white" />
        </shape></item>
    <item><shape>
            <stroke android:width="0.5dip" android:color="@color/contents_color" />

            <corners android:radius="5dp" />

            <solid android:color="@color/white" />
        </shape></item>

</selector>